# OpenAI API Configuration
# Required for both LangGraph agents and AutoGen 6-agent trading system
OPENAI_API_KEY=your_openai_api_key_here

# Alpha Vantage API Configuration (Optional)
# Used by OrganiserAgent for market data and technical indicators
# Get free API key at: https://www.alphavantage.co/support/#api-key
ALPHA_VANTAGE_API_KEY=your_alphavantage_api_key_here

# Tavily Search API Configuration (Optional)
# Used by DataAnalyst for web search and news research
# Get API key at: https://tavily.com
TAVILY_API_KEY=your_tavily_api_key_here

# Portfolio Configuration (Optional - can be set in code)
PORTFOLIO_VALUE=100000
RISK_PER_TRADE=2.0

# AutoGen Model Configuration (Optional)
AUTOGEN_MODEL=gpt-4o-mini
AUTOGEN_MAX_TURNS=20
